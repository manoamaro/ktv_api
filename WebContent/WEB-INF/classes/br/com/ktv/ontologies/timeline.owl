    </owl:Class>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#RelativeTimeLine -->

    <owl:Class rdf:about="http://purl.org/NET/c4dm/timeline.owl#RelativeTimeLine">
        <rdfs:label>relative timeline</rdfs:label>
