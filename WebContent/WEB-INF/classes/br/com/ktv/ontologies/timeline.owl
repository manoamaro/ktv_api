    <!-- http://purl.org/NET/c4dm/timeline.owl#ContinuousTimeLine -->

    <owl:Class rdf:about="http://purl.org/NET/c4dm/timeline.owl#ContinuousTimeLine">
        <rdfs:label>continuous timeline</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#PhysicalTimeLine"/>
