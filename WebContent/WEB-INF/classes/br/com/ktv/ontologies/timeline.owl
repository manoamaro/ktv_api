<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY time "http://www.w3.org/2006/time#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY daml "http://www.daml.org/2001/03/daml+oil#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY vs "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
]>


<rdf:RDF xmlns="http://purl.org/NET/c4dm/timeline.owl#"
     xml:base="http://purl.org/NET/c4dm/timeline.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/NET/c4dm/timeline.owl">
        <rdfs:label>The Timeline ontology</rdfs:label>
        <rdfs:comment>
    	Extends owl-time ontology (http://www.w3.org/2006/time) with  
	support for several timelines, acting as a backbone to adress 
	time interval/instants. 

	Mainly designed with a multimedia use-case in mind.

	Copyright (c) Yves Raimond, Samer Abdallah, Centre for Digital Music, Queen Mary, University of London
	</rdfs:comment>
        <owl:imports rdf:resource="http://www.w3.org/2006/time"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time-entry"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&vs;term_status"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYearMonth -->

    <rdfs:Datatype rdf:about="&xsd;gYearMonth"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/timeline.owl#after -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#after">
        <owl:equivalentProperty rdf:resource="&time;intervalAfter"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#before -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#before">
        <owl:equivalentProperty rdf:resource="&time;intervalBefore"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#contains -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#contains">
        <owl:equivalentProperty rdf:resource="&time;intervalContains"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#domainTimeLine -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#domainTimeLine">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>domain timeline</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>associates a timeline map to its domain timeline</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLine"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLineMap"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#during -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#during">
        <owl:equivalentProperty rdf:resource="&time;intervalDuring"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#equals -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#equals">
        <owl:equivalentProperty rdf:resource="&time;intervalEquals"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#finishedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#finishedBy">
        <owl:equivalentProperty rdf:resource="&time;intervalFinishedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#finishes -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#finishes">
        <owl:equivalentProperty rdf:resource="&time;intervalFinishes"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#meets -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#meets">
        <owl:equivalentProperty rdf:resource="&time;intervalMeets"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#metBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#metBy">
        <owl:equivalentProperty rdf:resource="&time;intervalMetBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#onTimeLine -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#onTimeLine">
        <vs:term_status>deprecated</vs:term_status>
        <owl:equivalentProperty rdf:resource="http://purl.org/NET/c4dm/timeline.owl#timeline"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#overlappedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#overlappedBy">
        <owl:equivalentProperty rdf:resource="&time;intervalOverlappedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#overlaps -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#overlaps">
        <owl:equivalentProperty rdf:resource="&time;intervalOverlaps"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>range timeline</rdfs:label>
        <rdfs:comment>associates a timeline map to its range timeline</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLine"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLineMap"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#startedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#startedBy">
        <owl:equivalentProperty rdf:resource="&time;intervalStartedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#starts -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#starts">
        <owl:equivalentProperty rdf:resource="&time;intervalStarts"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#timeline -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#timeline">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>on timeline</rdfs:label>
        <rdfs:comment>
	Relates an interval or an instant to the timeline on which it is defined.

	The 29th of August, 2007 would be linked through this property to the universal timeline, whereas
	&quot;from 2s to 5s on this particular signal&quot; would be defined on the signal&apos; timeline.

    </rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLine"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#Instant"/>
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2006/time#intervalAfter -->

    <rdf:Description rdf:about="&time;intervalAfter"/>
    


    <!-- http://www.w3.org/2006/time#intervalBefore -->

    <rdf:Description rdf:about="&time;intervalBefore"/>
    


    <!-- http://www.w3.org/2006/time#intervalContains -->

    <rdf:Description rdf:about="&time;intervalContains"/>
    


    <!-- http://www.w3.org/2006/time#intervalDuring -->

    <rdf:Description rdf:about="&time;intervalDuring"/>
    


    <!-- http://www.w3.org/2006/time#intervalEquals -->

    <rdf:Description rdf:about="&time;intervalEquals"/>
    


    <!-- http://www.w3.org/2006/time#intervalFinishedBy -->

    <rdf:Description rdf:about="&time;intervalFinishedBy"/>
    


    <!-- http://www.w3.org/2006/time#intervalFinishes -->

    <rdf:Description rdf:about="&time;intervalFinishes"/>
    


    <!-- http://www.w3.org/2006/time#intervalMeets -->

    <rdf:Description rdf:about="&time;intervalMeets"/>
    


    <!-- http://www.w3.org/2006/time#intervalMetBy -->

    <rdf:Description rdf:about="&time;intervalMetBy"/>
    


    <!-- http://www.w3.org/2006/time#intervalOverlappedBy -->

    <rdf:Description rdf:about="&time;intervalOverlappedBy"/>
    


    <!-- http://www.w3.org/2006/time#intervalOverlaps -->

    <rdf:Description rdf:about="&time;intervalOverlaps"/>
    


    <!-- http://www.w3.org/2006/time#intervalStartedBy -->

    <rdf:Description rdf:about="&time;intervalStartedBy"/>
    


    <!-- http://www.w3.org/2006/time#intervalStarts -->

    <rdf:Description rdf:about="&time;intervalStarts"/>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->

    <owl:ObjectProperty rdf:about="&foaf;maker"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/timeline.owl#at -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#at">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>at</rdfs:label>
        <rdfs:comment>refers to a point or an interval on the time line, through an explicit datatype</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#Instant"/>
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atDate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atDate">
        <rdfs:label>at (date)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :at, allowing to address a date (beginning of it for an instant, all of it for an interval)</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atDateTime -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atDateTime">
        <rdfs:label>at date/time</rdfs:label>
        <rdfs:comment>This property links an instant defined on the universal time line to an XSD date/time value</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atDuration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atDuration">
        <rdfs:label>at (duration)</rdfs:label>
        <rdfs:comment>A property enabling to adress a time point P through the duration of the interval [0,P] on a continuous timeline</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atInt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atInt">
        <rdfs:label>at (integer)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :at, having as a specific range xsd:int</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atReal -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atReal">
        <rdfs:label>at (real)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>subproperty of :at, having xsd:float as a range</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atYear -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atYear">
        <rdfs:label>at (year)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :at, allowing to address a year (beginning of it for an instant, all of it for an interval)</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;gYear"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#atYearMonth -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#atYearMonth">
        <rdfs:label>at (year/month)</rdfs:label>
        <rdfs:comment>A subproperty of :at, allowing to address a year/month (beginning of it for an instant, all of it for an interval)</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#at"/>
        <rdfs:range rdf:resource="&xsd;gYearMonth"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#beginsAt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#beginsAt">
        <vs:term_status>deprecated</vs:term_status>
        <owl:equivalentProperty rdf:resource="http://purl.org/NET/c4dm/timeline.owl#start"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#beginsAtDateTime -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#beginsAtDateTime">
        <rdfs:label>begins at (date/time)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :beginsAt, allowing to address the beginning of an interval as a date/time</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#start"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#beginsAtDuration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#beginsAtDuration">
        <rdfs:label>begins at (xsd:duration)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A property enabling to adress a start time point P of an interval [P,E] through the duration of the interval [0,P] on a continuous timeline</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#start"/>
        <rdfs:range rdf:resource="&xsd;duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#beginsAtInt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#beginsAtInt">
        <rdfs:label>begins at (integer)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :beginsAt, having xsd:int as a range</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#start"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#delay -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#delay">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>delay</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>associate a shift map to a particular delay</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#ShiftMap"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#duration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#duration">
        <rdfs:label>duration</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>the duration of a time interval</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#durationInt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#durationInt">
        <rdfs:label>duration (integer)</rdfs:label>
        <rdfs:comment>A subproperty of :duration, having xsd:int as a range</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#duration"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#durationXSD -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#durationXSD">
        <rdfs:label>duration (xsd:duration)</rdfs:label>
        <rdfs:comment>A subproperty of :duration, having xsd:duration as a range</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#duration"/>
        <rdfs:range rdf:resource="&xsd;duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#end -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#end">
        <rdfs:label>ends at</rdfs:label>
        <rdfs:comment>refers to the end of a time interval, through an explicit datatype. time:hasEnd can be used as well, if you want to associate the end of the interval to an explicit time point resource</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
        <owl:equivalentProperty rdf:resource="http://purl.org/NET/c4dm/timeline.owl#endsAt"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#endsAt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#endsAt">
        <vs:term_status>deprecated</vs:term_status>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#endsAtDateTime -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#endsAtDateTime">
        <rdfs:label>ends at (date/time)</rdfs:label>
        <rdfs:comment>A subproperty of :endsAt, allowing to address the end of an interval as a date/time</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#end"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#endsAtDuration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#endsAtDuration">
        <rdfs:label>ends at (xsd:duration)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A property enabling to adress an end time point P of an interval [S,P] through the duration of the interval [0,P] on a continuous timeline</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#end"/>
        <rdfs:range rdf:resource="&xsd;duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#endsAtInt -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#endsAtInt">
        <rdfs:label>ends at (integer)</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>A subproperty of :endsAt, having xsd:int as a range</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/timeline.owl#end"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#hopSize -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#hopSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>hop size</rdfs:label>
        <rdfs:comment>hop size, associated to a uniform windowing map</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformSamplingWindowingMap"/>
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformWindowingMap"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#origin -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#origin">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>origin</rdfs:label>
        <rdfs:comment>associate an origin map to its origin on the domain physical timeline</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#OriginMap"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#sampleRate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#sampleRate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>sample rate</rdfs:label>
        <rdfs:comment>associates a sample rate value to a uniform sampling map</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformSamplingMap"/>
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformSamplingWindowingMap"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#start -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#start">
        <rdfs:label>begins at</rdfs:label>
        <vs:term_status>stable</vs:term_status>
        <rdfs:comment>refers to the beginning of a time interval, through an explicit datatype. time:hasBeginning can be used as well, if you want to associate the beginning of the interval to an explicit time point resource</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#windowLength -->

    <owl:DatatypeProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#windowLength">
