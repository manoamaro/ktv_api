        <rdfs:comment>window length, associated to a uniform windowing map</rdfs:comment>
        <vs:term_status>stable</vs:term_status>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformSamplingWindowingMap"/>
                    <rdf:Description rdf:about="http://purl.org/NET/c4dm/timeline.owl#UniformWindowingMap"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


