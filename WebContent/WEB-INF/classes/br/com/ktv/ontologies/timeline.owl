                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/NET/c4dm/timeline.owl#timeline"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/NET/c4dm/timeline.owl#RelativeTimeLine"/>
